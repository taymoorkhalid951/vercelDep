{
    "version": 2,
    "builds": [
      {
        "src": "server/server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/",
        "dest": "/index.html"
      },
      {
        "src": "/api/(.*)",
        "dest": "/server/server.js"
      }
    ],
    "functions": {
      "server/server.js": {
        "memory": 1024,
        "maxDuration": 10
      }
    }
  }
  