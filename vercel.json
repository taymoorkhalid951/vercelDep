{
    "version": 2,
    "builds": [
      { "src": "server/server.js", "use": "@vercel/node" }
    ],
    "routes": [
      { "handle": "filesystem" },
      {
        "src": "/server/(.*)",
        "dest": "server/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ]
  }
  